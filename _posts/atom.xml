---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>DataJar Notes</title>
	<link href="http://notes.datajar.ca/post/atom.xml" rel="self"/>
	<link href="http://notes.datajar.ca"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://notes.datajar.ca</id>
	<author>
		<name>Jared Prins</name>
		<email>dj@datajar.ca</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://notes.datajar.ca{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://notes.datajar.ca{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
